{
  "problemMatcher": [
      {
          "owner": "exunit",
          "pattern": [
              {
                  "regexp": "^ +(.*):([0-9]+)$",
                  "file": 1,
                  "line": 2
              },
              {
                  "regexp": "^ +(.*)$",
                  "message": 1,
              }
          ]
      }
  ]
}